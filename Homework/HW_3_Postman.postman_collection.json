							"});\r",
							"\r",
							"pm.test(\"salary*3\", function(){\r",
							"    pm.expect(salary_env*coeff_2).to.eql(+response.salary[2])\r",
							"});\r",
							"\r",
							"//проверить, что 2-й элемент массива salary больше 1-го и 0-го\r",
							"\r",
							"pm.test(\"salary_2 > 0\", function(){\r",
							"    pm.expect(+response.salary[2]).to.be.above(response.salary[0])\r",
							"});\r",
							"pm.test(\"salary_2 > 1\", function(){\r",
							"    pm.expect(+response.salary[2]).to.be.above(+response.salary[1])\r",
							"});"
						],
